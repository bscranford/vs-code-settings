{
    "workbench.iconTheme": "city-lights-icons-vsc-light",
    "workbench.colorTheme": "Dracula At Night",
    "window.zoomLevel": 1,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Fira Code', Menlo, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.wordWrap": "on",
